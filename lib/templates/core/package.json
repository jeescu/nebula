{
  "name": "Nebula API",
  "version": "0.0.0",
  "description": "API and micro-service generator",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "",
  "author": "",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.18.0",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "errorhandler": "^1.4.3",
    "express": "^4.13.3",
    "helmet": "^3.1.0",
    "jsonwebtoken": "^7.1.0",
    "knex": "^0.12.6",
    "lodash": "^4.17.4",
    "method-override": "^2.3.6",
    "mysql": "^2.9.0",
    "objection": "^0.6.2"
  },
  "devDependencies": {
    "async": "^2.1.4",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2",
    "q": "^1.4.1"
  }
}
